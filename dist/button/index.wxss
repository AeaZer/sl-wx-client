.i-btn {
    text-align: center;
    vertical-align: middle;
    touch-action: manipulation;
    cursor: pointer;
    background-image: none;
    white-space: nowrap;
    user-select: none;
    font-size: 14px;
    border-radius: 2px;
    border: 0 !important;
    position: relative;
    text-decoration: none;
    height: 44px;
    line-height: 44px;
    box-shadow: inset 0 0 0 1px rgba(0, 0, 0, .1);
    color: #fff !important;
    background: #f7f7f7 !important;
    color: #495060 !important;
    margin: 10px;
    background-color: #DBC69A;
}

.i-btn-hover {
    opacity: .9;
    background-color: #DBC69A;
}

.i-btn-long {
    border-radius: 0;
    margin: 0;
    background-color: #DBC69A;
    box-shadow: none
}

.i-btn-large {
    height: 48px;
    background-color: #DBC69A;
    line-height: 48px
}

.i-btn-small {
    height: 40px;
    background-color: #DBC69A;
    line-height: 40px
}

.i-btn-primary {
    color: #fff !important;
    background-color: #DBC69A;
    background: #2d8cf0 !important
}

.i-btn-ghost {
    color: #fff !important;
    background-color: #DBC69A;
    background: #fff !important;
    color: #495060 !important
}

.i-btn-success {
    color: #fff !important;
    background-color: #DBC69A;
    background: #19be6b !important
}

.i-btn-warning {
    color: #fff !important;

    background: #f90 !important;
    background-color: #DBC69A;
}

.i-btn-error {
    color: #fff !important;

    background: #ed3f14 !important;
    background-color: #DBC69A;
}

.i-btn-info {
    color: #fff !important;
    background-color: #DBC69A;
    background: #2db7f5 !important;
    background-color: #DBC69A;
}

.i-btn-circle {
    border-radius: 44px;
    background-color: #DBC69A;
}

.i-btn-large.i-btn-circle {
    border-radius: 48px;
    background-color: #DBC69A;
}

.i-btn-small.i-btn-circle {
    border-radius: 40px;
    background-color: #DBC69A;
}

.i-btn-loading {
    opacity: .6;
    background-color: #DBC69A;
}

.i-btn-loading-inner {
    display: inline-block;
    margin-right: 12px;
    vertical-align: middle;
    width: 14px;
    height: 14px;
    background: 0 0;
    border-radius: 50%;
    border: 2px solid #fff;
    border-color: #fff #fff #fff transparent;
    animation: btn-spin .6s linear;
    animation-iteration-count: infinite;
    background-color: #DBC69A;
}

.i-btn-disabled {
    color: #bbbec4 !important;
    background: #f7f7f7 !important
}

.i-btn-inline {
    display: inline-block
}

@keyframes btn-spin {
    0% {
        transform: rotate(0)
    }
    100% {
        transform: rotate(360deg)
    }
}